-------------------------------------------------------------------------------- PAGE 4 --------------------------------------------------------------------------------

Page4.Button({
    Text = "Craft Mag Armor",
    Callback = function()

        local args = {
            [1] = "Magnetite Mask"
        }
        
        game:GetService("ReplicatedStorage").Events.CraftItem:FireServer(unpack(args))
        
        local args = {
            [1] = "Magnetite Chestplate"
        }
        
        game:GetService("ReplicatedStorage").Events.CraftItem:FireServer(unpack(args))
         
        local args = {
            [1] = "Magnetite Greaves"
        }
        
        game:GetService("ReplicatedStorage").Events.CraftItem:FireServer(unpack(args))
        
    end
})

